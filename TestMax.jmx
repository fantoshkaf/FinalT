              <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Extract_Prod_Color" enabled="true">
                <stringProp name="scriptLanguage">groovy</stringProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="cacheKey">false</stringProp>
                <stringProp name="script">import java.util.Random

def prodLen = Integer.parseInt(vars.get(&quot;products_matchNr&quot;))
def random = new Random()

def product = null
def colorCode = null
def maxAttempts = 100 
def attempts = 0 

do {

    def randomProdIndex = random.nextInt(prodLen) + 1
    def prodJson = vars.get(&quot;products_$randomProdIndex&quot;)

    if (prodJson) {
        def prod = new groovy.json.JsonSlurper().parseText(prodJson)

        if (prod.colors &amp;&amp; prod.colors.size() &gt; 0) {
            def colorWithStock = prod.colors.findAll { it.inStock &gt; 0 &amp;&amp; !it.code.startsWith(&quot;#&quot;) }
        		if (colorWithStock) {
        			def randomColor = colorWithStock[new Random().nextInt(colorWithStock.size())]
    				 colorCode = randomColor.code
        			def inStock = randomColor.inStock
        			if (inStock &gt; 0) {
            		vars.put(&quot;color&quot;, colorCode)
            		vars.put(&quot;product&quot;, prod.productId.toString())
            break}}
        }
    }
    
    attempts++
} while (attempts &lt; maxAttempts)


if (colorCode != null &amp;&amp; product != null) {
    vars.put(&quot;color&quot;, colorCode as String)
    vars.put(&quot;product&quot;, product as String)
    log.info(&quot;Выбранный продукт: $product, цвет: $colorCode&quot;)
} </stringProp>
              </JSR223PostProcessor>
