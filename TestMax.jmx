                <stringProp name="script">import groovy.json.JsonSlurper

// Получаем текущий ответ в формате JSON
def response = new JsonSlurper().parseText(prev.getResponseDataAsString())

// Получаем значения извлеченных переменных
def extractedProductId = vars.get(&quot;delproductId&quot;)
def extractedColorCode = vars.get(&quot;delColor&quot;)

// Проверка наличия значений переменных
log.info(&quot;Извлеченный productId: &quot; + extractedProductId)
log.info(&quot;Извлеченный colorCode: &quot; + extractedColorCode)

// Преобразование переменных в соответствующие типы данных
extractedProductId = extractedProductId as Integer
extractedColorCode = extractedColorCode as String

// Извлекаем значения productId и color.code для каждого продукта в productsInCart
def matchingProduct = response.productsInCart.find { 
    it.productId == extractedProductId &amp;&amp; it.color.code == extractedColorCode
}

// Проверка наличия совпадений
if (matchingProduct) {
    AssertionResult.setFailure(true)
    AssertionResult.setFailureMessage(&quot;Найден продукт с совпадающими productId и color.code&quot;)
    log.error(&quot;Найден продукт с совпадающими значениями: productId = ${extractedProductId}, colorCode = ${extractedColorCode}&quot;)
} else {
    log.info(&quot;Совпадений не найдено&quot;)
}</stringProp>
