                <stringProp name="cacheKey">true</stringProp>
                <stringProp name="script">import java.util.Random

def prodLen = Integer.parseInt(vars.get(&quot;AllProds_matchNr&quot;))
def prodLen2 = Integer.parseInt(vars.get(&quot;products_matchNr&quot;))
def random = new Random()
def maxAttempts = 100
def attempts = 0

do {
    def randomProdIndex = random.nextInt(prodLen2) + 1
    def prodRand = vars.get(&quot;products_$randomProdIndex&quot;) // случайный продукт из найденных
    def prodSel = new groovy.json.JsonSlurper().parseText(prodRand)
    def ie = prodSel.get(&quot;productId&quot;)

    if (prodSel.get(&quot;productId&quot;) &lt; 14) {
        def prodAll
        for (int i = 1; i &lt;= prodLen; ++i) {
            def prodJson = vars.get(&quot;AllProds_$i&quot;)
            prodAll = new groovy.json.JsonSlurper().parseText(prodJson)

            if (prodSel.productId == prodAll.productId) {
                break
            }
        }

			def colorWithStock = prodAll.colors.findAll { it.inStock &gt; 0 &amp;&amp; !it.code.startsWith(&quot;#&quot;) }

        		if (colorWithStock) {
        			def randomColor = colorWithStock[new Random().nextInt(colorWithStock.size())]
    				def colorCode = randomColor.code
        			def inStock = randomColor.inStock
        			if (inStock &gt; 0) {
            		vars.put(&quot;color&quot;, colorCode)
            		vars.put(&quot;product&quot;, prodAll.productId.toString())
            break}
                            else {
            attempts++
        }
        }
      
        else {
            attempts++
        }
    } else {
        def prodJson = vars.get(&quot;AllProds_$ie&quot;)
        def prodAll = new groovy.json.JsonSlurper().parseText(prodJson)

			def colorWithStock = prodAll.colors.findAll { it.inStock &gt; 0 &amp;&amp; !it.code.startsWith(&quot;#&quot;) }

        		if (colorWithStock) {
        			def randomColor = colorWithStock[new Random().nextInt(colorWithStock.size())]
    				def colorCode = randomColor.code
        			def inStock = randomColor.inStock
        			if (inStock &gt; 0) {
            		vars.put(&quot;color&quot;, colorCode)
            		vars.put(&quot;product&quot;, prodAll.productId.toString())
            break}
                            else {
            attempts++
        }
        }
      
        else {
            attempts++
        }
    }
} while (attempts &lt; maxAttempts)</stringProp>
